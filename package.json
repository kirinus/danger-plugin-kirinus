{
  "name": "danger-plugin-kirinus",
  "description": "Set of Danger rules used to check Kirinus Digital PRs.",
  "author": {
    "name": "Dario Blanco",
    "email": "development@kirinus.digital"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kirinus/danger-plugin-kirinus.git"
  },
  "bugs": {
    "url": "https://github.com/kirinus/danger-plugin-kirinus/issues"
  },
  "homepage": "https://github.com/kirinus/danger-plugin-kirinus#readme",
  "keywords": [
    "danger",
    "danger-plugin",
    "kirinus"
  ],
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "build": "tsc",
    "docs": "esdoc -c .esdoc.json",
    "lint": "eslint ./src/**/*.ts",
    "predocs": "rm -rf docs/",
    "prepare": "yes | yarn husky install",
    "test": "jest"
  },
  "license": "MIT",
  "engines": {
    "node": ">=7.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/jest": "^19.2.4",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "danger": "*",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-ordered-imports": "^0.5.0",
    "husky": "^6.0.0",
    "jest": "^20.0.1",
    "lint-staged": "^3.4.1",
    "prettier": "^2.3.0",
    "prettier-eslint": "^12.0.0",
    "pretty-quick": "^3.1.0",
    "ts-jest": "^20.0.0",
    "typescript": "^4.3.4"
  },
  "optionalDependencies": {
    "esdoc": "^0.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(.test)\\.(ts|tsx)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ]
  }
}
